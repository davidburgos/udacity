<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextStyle" parent="@android:style/TextAppearance.Medium">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:textColor">#000000</item>
    </style>
    <style name="TextStyle.Title">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/abc_text_size_title_material</item>
    </style>
    <style name="TextStyle.ButtonStyle">
        <item name="android:textStyle">normal</item>
        <item name="android:background">#F08C35</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:textSize">@dimen/abc_text_size_title_material</item>
    </style>
</resources>